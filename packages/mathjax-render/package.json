{
  "name": "mathjax-render",
  "version": "0.0.4",
  "license": "MIT",
  "author": "Dup4 <lyuzhi.pan@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dup4/mathjax-render.git",
    "directory": "packages/mathjax-render"
  },
  "bugs": {
    "url": "https://github.com/Dup4/mathjax-render/issues"
  },
  "homepage": "https://github.com/Dup4/mathjax-render/tree/main/packages/mathjax-render#readme",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "browser": {
    "./dist/index.cjs": "./dist/esm/browser/index.ts",
    "./dist/index.d.ts": "./dist/esm/browser/index.d.ts",
    "./dist/index.mjs": "./dist/esm/browser/index.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:node": "unbuild",
    "build:browser": "cross-env BROWSER=1 unbuild",
    "dev": "unbuild --stub",
    "start": "esmo src/index.ts",
    "test": "vitest",
    "lint:build": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "escape-html": "^1.0.3",
    "jsdom": "^19.0.0",
    "mathjax-full": "3.2.0"
  },
  "devDependencies": {
    "@types/escape-html": "^1.0.2",
    "@types/jsdom": "^16.2.14",
    "cross-env": "^7.0.3",
    "glob": "^8.0.3"
  }
}
