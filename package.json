{
  "private": true,
  "version": "0.0.18",
  "packageManager": "pnpm@8.6.6",
  "license": "MIT",
  "author": "Dup4 <lyuzhi.pan@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dup4/mathjax-render.git"
  },
  "homepage": "https://github.com/Dup4/mathjax-render#readme",
  "bugs": {
    "url": "https://github.com/Dup4/mathjax-render/issues"
  },
  "scripts": {
    "build": "pnpm -r --filter=./packages/* run build",
    "dev": "pnpm -r --parallel --filter=./packages/* run dev",
    "start": "pnpm -r --parallel --filter=./packages/* run start",
    "test": "vitest",
    "lint": "run-p lint:*",
    "lint:build": "pnpm -r --parallel --filter=./packages/* run lint:build",
    "lint:eslint": "eslint .",
    "lint:style": "prettier --check ./packages/**/*.ts",
    "format": "run-p format:*",
    "format:eslint": "eslint --fix .",
    "format:style": "prettier --write ./packages/**/*.ts",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "bumpp": "^7.2.0",
    "eslint": "^8.44.0",
    "esmo": "^0.14.1",
    "fsxx": "^0.0.5",
    "glob": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.6.6",
    "prettier": "^2.8.8",
    "taze": "^0.10.3",
    "typescript": "^4.9.5",
    "unbuild": "^0.7.6",
    "vite": "^2.9.16",
    "vitest": "^0.7.13",
    "zx": "^6.2.5"
  }
}
